#include <stdio.h>

int main() {
    int num1, num2;

    scanf("%d", &num1);
    scanf("%d", &num2);

    printf("%d\n", num1 + num2);   // Addition
    printf("%d\n", num1 - num2);   // Subtraction
    printf("%d\n", num1 * num2);   // Multiplication
    printf("%d\n", num1 % num2);   // Modulo division
    printf("%d\n", num1 / num2);   // Division

    return 0;
}
